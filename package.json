    "constructs": "^10.0"
  },
  "devDependencies": {
    "@microsoft/api-documenter": "7.28.2",
    "@microsoft/api-extractor": "7.55.2",
    "@rollup/plugin-typescript": "12.3.0",
    "@types/node": "24.10.7",
    "aws-cdk-lib": "2.234.1",
    "constructs": "10.4.4",
    "npm-run-all2": "^8.0.4",
    "rimraf": "^6.1.2",
    "rollup": "^4.55.1",
    "tslib": "2.8.1",
    "typescript": "5.8.2"
