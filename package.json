{
  "name": "@codemonger-io/cdk-ghost-string-parameter",
  "version": "0.2.1",
  "description": "String parameter in Parameter Store on AWS Systems Manager",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/index.js",
    "dist/index.js.map",
    "dist/index.d.ts"
  ],
  "scripts": {
    "build": "rimraf dist && rollup -c && api-extractor run --local",
